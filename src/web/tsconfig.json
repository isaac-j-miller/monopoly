{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "lib": ["scripthost", "dom", "dom.iterable", "es2019"],
    "outDir": "./dist",
    "types": ["node", "jest"],
    "typeRoots": ["../../node_modules/@types"],
    "module": "esnext",
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "include": ["src/**/*.ts", "src/**/*.tsx"],
  "exclude": ["node_modules", "./dist"]
}
